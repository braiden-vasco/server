server {
	listen 80      default_server;
	listen [::]:80 default_server;

	listen 443      ssl default_server;
	listen [::]:443 ssl default_server;

	ssl_certificate     /etc/ssl/public/braiden-vasco_network.crt-bundle;
	ssl_certificate_key /etc/ssl/private/braiden-vasco_network.key;

	root /var/www/html;

	index index.html index.htm index.nginx-debian.html;

	server_name _;

	location / {
		try_files $uri $uri/ =404;
	}
}
